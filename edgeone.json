{
    "headers": [
        {
            "source": "/*",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "Cache-Control",
                    "value": "max-age=1209600"
                },
            ]
        }
    ],
    "caches": [
        {
            "source": "/*",
            "cacheTtl": 604800
        }
    ],
    "rewrites": [
        {
            "source": "/FirPE-Docs/*",
            "destination": "/:splat"
        },
        {
            "source": "/*",
            "destination": "/docs/.vitepress/dist/:splat"
        }
    ]
}    
